@model UserViewModel
@{
    ViewData["Title"] = "Delete";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>
    Are you Sure to Delete  @Model.Fname    @Model.Lname
    
</h1>

<div asp-vaildation-summery="All">
</div>
<form asp-action="ConfirmDelete">


        <div class="form-group">
            <label asp-for="Id" class="col-form-label"></label>
            <input readonly asp-for="Id" class="form-control" />

        </div>
        <div class="form-group">
            <label asp-for="Fname" class="col-form-label"></label>
            <input readonly asp-for="Fname" class="form-control" />

        </div>
        <div class="form-group">
            <label asp-for="Lname" class="col-form-label"></label>
            <input readonly asp-for="Lname" class="form-control" />
        </div>
        <div class="form-group">
            <label asp-for="Email" class="col-form-label"></label>
            <input readonly asp-for="Email" class="form-control" />
        </div> 
        <div class="form-group">
            <label asp-for="PhoneNumber" class="col-form-label"></label>
            <input readonly asp-for="PhoneNumber" class="form-control" />
        </div>

        <input value="Delete" class="btn btn-danger" type="submit">
        <a asp-controller="User" asp-action="Index" class="btn btn-light">Back To List</a>
  
</form>